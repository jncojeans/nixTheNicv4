{
  "expo": {
    "name": "Nix the Nic",
    "slug": "nixthenic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1a",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kickthenic.app",
      "buildNumber": "24",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "NSSupportsLiveActivities": true
      },
      "associatedDomains": [],
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.kickthenic.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1a1a1a"
        }
      ],
      [
        "expo-background-fetch",
        {
          "netCacheDirectory": "net_cache"
        }
      ],
      "./plugins/withLiveActivities.js",
      [
        "@bacons/apple-targets",
        {
          "targets": [
            {
              "name": "PouchWidgetExtension",
              "type": "widget-extension",
              "bundleId": "com.kickthenic.app.PouchWidgetExtension",
              "entitlements": {
                "com.apple.security.application-groups": ["group.com.kickthenic.app"]
              },
              "deploymentTarget": "16.2",
              "frameworks": ["SwiftUI", "ActivityKit"]
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "40517342-2c9d-41a8-8d96-464b79eef360"
      }
    },
    "owner": "jncojeans",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/40517342-2c9d-41a8-8d96-464b79eef360"
    }
  }
}
